import java.util.Scanner;

public class Uppgift8 {

	public static void main(String[] args) {
		
		Scanner scanner = new Scanner(System.in);
		
		int år;
		
		System.out.print("Skriv in ett årtal: ");
		
		år = scanner.nextInt();
		
		if(år % 4 == 0){
			System.out.println(år + " är ett skottår!");
		}
		

	}

}
